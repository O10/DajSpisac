<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="@color/white" />
            <stroke
                android:width="3dp"
                android:color="@color/orangeDajSpisac" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="@color/white" />
            <stroke
                android:width="3dp"
                android:color="@color/lightBlueDajSpisac" />
            <corners
                android:radius="10dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
</selector>